<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:lab6_login">
    <Style x:Key="Standard">
        <Setter Property="Button.FontSize" Value="12" />
        <Setter Property="Button.FontWeight" Value="Regular" />
        <Setter Property="TextBox.FontSize" Value="12" />
        <Setter Property="TextBox.FontWeight" Value="Regular" />
        <Setter Property="Label.FontSize" Value="12" />
        <Setter Property="Label.FontWeight" Value="Regular" />
    </Style>
    <Style x:Key="WindowStyle" TargetType="Window">
        <Setter Property="Background" Value="White" />
    </Style>
</ResourceDictionary>